{
  "expo": {
    "name": "zikirsayac",
    "slug": "zikirsayac",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-screen.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#1E40AF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#4FD1C7"
      },
      "edgeToEdgeEnabled": false,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "POST_NOTIFICATIONS",
        "SCHEDULE_EXACT_ALARM"
      ],
      "package": "com.celilcavus.zikirsayac",
      "versionCode": 1,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.celilcavus.zikirsayac"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#4FD1C7",
          "sounds": []
        }
      ],
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "f945a7bc-1e74-424c-b474-75c8d15d561e"
      }
    }
  }
}
